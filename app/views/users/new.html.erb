<h1>Signup</h1>

<div class="panel panel-default products">
  <%# <table class="table-signup">
    <tbody> %>

<%= form_for :user, url: '/users' do |f| %>
<div class="form-group">
  <p>First Name: <%= f.text_field :first_name %></p><br>
  <p>Last Name: <%= f.text_field :last_name %></p><br>
  <p>Email: <%= f.text_field :email %></p><br>
  <p>Password: <%= f.password_field :password %></p><br>
  <p>Password Confirmation: <%= f.password_field :password_confirmation %></p><br>
  <%= f.submit "Submit" %>
</div>
<% end %>
    <%# </tbody>
  </table> %>
</div>
